<GUIItems>
	<Item name="Doctrine_Screen" sizeX="1290" sizeY="1380" hidden="true">
		<OnOpen>
			<Action type="PlaySound" target="gui_customize_equip" />
			<Action type="AddChild" target="Deploy_Screen" />
			<!--  we're doing this to allow DeployScreen to receive input, since this page captures
			input  -->
			<Action type="TriggerEvent" target="GUI_CAPTURE_INPUT" />
			<Action type="TriggerEvent" target="GUI_GAME_DOCTRINE_OPENED" />
			<!--  Add as child here, so that it gets hidden when we close the customization screen
			via game code. Note that Menu_Main_Blurred_Background keeps its own hidden/shown status,
			in case it was previously open, otherwise it stays hidden.
			We also depend on code to close the customization screen before showing the Squads screen and
			viceversa.
		 -->
			<Action type="AddChild" target="Menu_Main_Blurred_Background" />
			<Action type="MoveToBackground" target="Menu_Main_Blurred_Background" />
		</OnOpen>
		<OnClose>
			<Action type="TriggerEvent" target="GUI_RELEASE_INPUT" />
			<Action type="TriggerEvent" target="GUI_GAME_DOCTRINE_CLOSED" />
			<Action type="RemoveChild" target="Deploy_Screen" />
		</OnClose>
		<OnKeyDown key0="27" key1="32">
			<!--  Escape and Space  -->
			<Action type="Click" target="Doctrine_Screen_Back" />
		</OnKeyDown>
		<Item origin="0 0" sizeX="9999" sizeY="9999" blurBackground="true" blurColor="a0a0a0"> </Item>
		<!--  clicking on this (the background) will close the screen
		 note that the the "background" item below should also 'stealFocus'
	 -->
		<Item origin="0 0" sizeX="9999" sizeY="9999">
			<OnCursorDown>
				<Action type="Click" target="Doctrine_Screen_Back" />
			</OnCursorDown>
			<OnRClickDown>
				<Action type="Click" target="Doctrine_Screen_Back" />
			</OnRClickDown>
		</Item>
		<!--  total doctrine points available  -->
		<StaticImage name="#available_doctrinepoints_ttip" origin="170 30" align="rt"
			tooltip="change by game">
			<RenderObject2D texture="data/textures/gui/doctrines/customization_doctrine.dds" />
			<StaticText align="t" name="#available_doctrinepoints" origin="0 -120" text="32"
				font="header_3" textColor="f97b03" />
		</StaticImage>
		<!--  total battlehonors
	<StaticImage origin="170 -190" align="rt" tooltip="@customization_battlehonors_ttip">
		<RenderObject2D texture="data/textures/gui/customization/customization_bh.dds" />
		<StaticText align="t" name="#available_battlehonors" origin="0 -200" text="32" font="header_3"
		textColor="f97b03"/>
	</StaticImage>
	 -->
		<!--  background  -->
		<StaticImage stealFocus="true">
			<RenderObject2D texture="data/textures/gui/menu_darkbrown_background.tga" sizeX="1290"
				sizeY="1444" color="ffffffc8" />
			<StaticText text="@menu_m_doctrine" origin="24 674" align="l" font="header_2"
				textColor="f97b03" />
		</StaticImage>
		<Item name="#DoctrineTree_Parent" align="i">
			<!-- 
			Game code adds here the item named #UnitName_DoctrineTree
			eg <Item name="#Rangers_Doctrine"> ...

			Do not add anything else as child to this item.
		 -->
		</Item>
		<Button name="#Doctrine_Reset" align="rb" origin="-20 20">
			<ButtonText text="@menu_doctrine_reset" origin="-4 0" align="r" font="header_2"
				textColor="f0e3cc" />
			<OnHover>
				<ButtonText text="@menu_doctrine_reset" align="r" origin="-4 0" font="header_2"
					textColor="211e1d" />
				<RenderObject2D texture="data/textures/gui/button_hover_01.tga" align="l"
					color="f0e3cc" flipX="true" />
			</OnHover>
			<OnClick>
				<Action type="TriggerEvent" target="GUI_GAME_DOCTRINE_RESET" />
			</OnClick>
		</Button>
		<Button name="Doctrine_Screen_Back" align="lb" origin="20 20">
			<ButtonText text="@menu_generic_back" align="l" font="header_2" textColor="f0e3cc" />
			<OnHover>
				<ButtonText text="@menu_generic_back" align="l" font="header_2" textColor="211e1d" />
				<RenderObject2D texture="data/textures/gui/button_hover_01.tga" align="l"
					color="f0e3cc" />
			</OnHover>
			<OnClick>
				<!--  hides the doctrine screen and either goes back to Squads screen or to nothing
				(if in game and we're customizing the current squad)  -->
				<Action type="TriggerEvent" target="GUI_GAME_DOCTRINE_BACK" />
			</OnClick>
		</Button>
	</Item>
